<resources>
    <color name="colorPrimary">#1583ff</color>
    <color name="colorPrimaryDark">#126dd4</color>
    <color name="colorAccent">#c83a3a</color>
    <color name="keywordBackground">#e2e2e2</color>
    <color name="searchButton">#d2d2d2</color>
    <color name="searchText">#adadad</color>
    <color name="search_history_line">#0076fa</color>
    <!-- Screens background color-->
    <color name="bg_screen1">#f64c73</color>
    <color name="bg_screen2">#20d2bb</color>
    <color name="bg_screen3">#3395ff</color>
    <color name="bg_screen4">#c873f4</color>

    <!-- dots inactive colors -->
    <color name="dot_dark_screen1">#494949</color>
    <color name="dot_dark_screen2">#494949</color>
    <color name="dot_dark_screen3">#494949</color>
    <color name="dot_dark_screen4">#494949</color>

    <!-- dots active colors -->
    <color name="dot_light_screen1">#FFFFFF</color>
    <color name="dot_light_screen2">#FFFFFF</color>
    <color name="dot_light_screen3">#FFFFFF</color>
    <color name="dot_light_screen4">#FFFFFF</color>

    <array name="array_dot_active">
        <item>@color/dot_light_screen1</item>
        <item>@color/dot_light_screen2</item>
        <item>@color/dot_light_screen3</item>
        <item>@color/dot_light_screen4</item>
    </array>

    <array name="array_dot_inactive">
        <item>@color/dot_dark_screen1</item>
        <item>@color/dot_dark_screen2</item>
        <item>@color/dot_dark_screen3</item>
        <item>@color/dot_dark_screen4</item>
    </array>

